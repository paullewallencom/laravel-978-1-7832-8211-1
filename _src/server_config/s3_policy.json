{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::recipe-artifacts"
            ]
        },
        {
            "Sid": "Stmt1452005994000",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
